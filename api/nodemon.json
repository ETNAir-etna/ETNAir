{
  "watch": ["src"],
  "ignore": [],
  "ext": "ts",
  "exec": "ts-node src/server.ts",
  "signal": "SIGINT",
  "env": {
    "POSTGRES_USER": "etnair_user",
    "POSTGRES_PASSWORD": "etnair_pwd",
    "POSTGRES_DB": "etnair_db",
